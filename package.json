{
  "name": "lensroom-v2",
  "version": "1.0.0",
  "description": "AI Content Generation Platform - 12 лучших AI моделей для фото и видео",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start -p 3002",
    "lint": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:smoke:ui": "node scripts/smoke-ui.js",
    "test:smoke:video": "RUN_LIVE_SMOKE=1 SMOKE_ALLOW_CHARGE=0 npx tsx scripts/live-smoke-video.ts",
    "clean": "rm -rf .next out node_modules/.cache",
    "backfill:previews": "node scripts/backfill-previews.js",
    "worker:previews": "node scripts/previews-worker.js",
    "worker:previews:oneshot": "PREVIEWS_WORKER_ONESHOT=1 node scripts/previews-worker.js",
    "previews:rebuild": "node scripts/previews-rebuild.js",
    "previews:rebuild:dry": "node scripts/previews-rebuild.js --dry-run"
  },
  "dependencies": {
    "@fal-ai/client": "^1.8.4",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.87.1",
    "@tanstack/react-query": "^5.90.12",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/js-cookie": "^3.0.6",
    "@use-gesture/react": "^10.3.1",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^12.23.26",
    "jose": "^6.1.3",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "lucide-react": "^0.561.0",
    "next": "16.0.10",
    "react": "19.2.1",
    "react-confetti": "^6.4.0",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.68.0",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "web-push": "^3.6.7",
    "zod": "^4.3.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.0",
    "@playwright/test": "^1.58.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "^4.0.18",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "jsdom": "^27.4.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "puppeteer": "^24.35.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "LensRoom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/lensroom"
  },
  "keywords": [
    "ai",
    "image-generation",
    "video-generation",
    "flux",
    "sora",
    "kie.ai",
    "nextjs",
    "typescript"
  ]
}
